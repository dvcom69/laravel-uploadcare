{
    "name": "inwave/laravel-uploadcare",
    "description": "Laravel Serviceprovider for Uploadcare",
    "keywords": ["laravel", "uploadcare"],
    "homepage":"https://github.com/InWave/laravel-uploadcare.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Overtoom",
            "email": "kevin@inwave.nl",
            "homepage": "http://www.inwave.nl",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git://github.com/uploadcare/uploadcare-php.git"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": ">=5.5.0",
        "laravelcollective/html": ">=5.5.0",
        "uploadcare/uploadcare-php": "v2.3.0"
    },
    "autoload": {
        "psr-0": {
            "Inwave\\LaravelUploadcare": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Inwave\\LaravelUploadcare\\LaravelUploadcareServiceProvider"
            ],
            "aliases": {
                "Uploadcare": "Inwave\\LaravelUploadcare\\Facades\\Uploadcare"
            }
        }
    }
}
